{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,

        "import/extensions": [
            "error",
            "ignorePackages"
        ]

    }
}
